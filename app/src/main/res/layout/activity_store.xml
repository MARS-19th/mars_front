            <ImageView
                android:id="@+id/etcImage"
                android:layout_width="0dp"
                android:layout_height="40dp"
                android:layout_weight="1"
                android:paddingVertical="8dp"
                android:scaleType="fitCenter"
                android:src="@drawable/icon_store3" />

        </LinearLayout>

        <LinearLayout
